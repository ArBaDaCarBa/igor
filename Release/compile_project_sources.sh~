#!/bin/bash
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Aligner.d" -MT"generative_model_src/Aligner.d" -o "generative_model_src/Aligner.o" "../generative_model_src/Aligner.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Deletion.d" -MT"generative_model_src/Deletion.d" -o "generative_model_src/Deletion.o" "../generative_model_src/Deletion.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Dinuclmarkov.d" -MT"generative_model_src/Dinuclmarkov.d" -o "generative_model_src/Dinuclmarkov.o" "../generative_model_src/Dinuclmarkov.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Errorrate.d" -MT"generative_model_src/Errorrate.d" -o "generative_model_src/Errorrate.o" "../generative_model_src/Errorrate.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/GenModel.d" -MT"generative_model_src/GenModel.d" -o "generative_model_src/GenModel.o" "../generative_model_src/GenModel.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Genechoice.d" -MT"generative_model_src/Genechoice.d" -o "generative_model_src/Genechoice.o" "../generative_model_src/Genechoice.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Insertion.d" -MT"generative_model_src/Insertion.d" -o "generative_model_src/Insertion.o" "../generative_model_src/Insertion.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Model_Parms.d" -MT"generative_model_src/Model_Parms.d" -o "generative_model_src/Model_Parms.o" "../generative_model_src/Model_Parms.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Model_marginals.d" -MT"generative_model_src/Model_marginals.d" -o "generative_model_src/Model_marginals.o" "../generative_model_src/Model_marginals.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Rec_Event.d" -MT"generative_model_src/Rec_Event.d" -o "generative_model_src/Rec_Event.o" "../generative_model_src/Rec_Event.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Singleerrorrate.d" -MT"generative_model_src/Singleerrorrate.d" -o "generative_model_src/Singleerrorrate.o" "../generative_model_src/Singleerrorrate.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/Utils.d" -MT"generative_model_src/Utils.d" -o "generative_model_src/Utils.o" "../generative_model_src/Utils.cpp"
g++ -std=c++1y -O3 -Wall -c -fmessage-length=0 -fopenmp -MMD -MP -MF"generative_model_src/main.d" -MT"generative_model_src/main.d" -o "generative_model_src/main.o" "../generative_model_src/main.cpp"
